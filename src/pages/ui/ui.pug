extends ../layout

append script
  script(src=require('./ui.js'))
  //- script(src=require('../../modules/blocks/dropdown-rooms/dropdown-rooms.js'))
  //- script(src=require('../../modules/blocks/expandable-checkbox-list/dropdown-checkbox.js'))

append style
    link(rel='stylesheet' href=require('./ui.scss'))


include ../mixins


block content
  a(href='index.html')
      button.to-home < Home 
  .wrapper
      h2.ui-info Color & Type
      article.colors.ui-block
        include ./ui-parts/colors
      h2.ui-info Form Elements
      article.elements.ui-block
        include ./ui-parts/elements
      h2.ui-info Headers & Footers
      +headImgGrey
      article.headers-footers
        include ./ui-parts/headersAndFooters