mixin findRooms
    .find-rooms 
        h1 Найдём номера под ваши пожелания
        .find-rooms__datepicker
            +datePicker({calendarId: 'cards-two-dates-cal', leftTitle: 'прибытие', rightTitle: 'выезд', datePickerId: 'date-picker-cards-first', type: 'two-input'})
        .find-rooms__guests-dropdown.js-main__guest-dropdown
            +dropdownGuest({title: 'гости', id: 'guests-cards'})
        .find-rooms__button 
            +registerButton({text:'подобрать номер', arrow:true})